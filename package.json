{
  "name": "reactive",
  "version": "0.0.3",
  "author": "Simen Brekken <simen@unfold.no>",
  "description": "React client/server example",
  "license": "MIT",
  "engines": {
    "node": "0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unfold/reactive"
  },
  "main": "server.js",
  "browser": "./client.js",
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "scripts": {
    "start": "node server",
    "test": "npm run test-server && npm run test-client",
    "test-server": "node_modules/.bin/mocha --require should --reporter spec test/server.js",
    "test-client": "node test/build-client-test-bundle > test/client.bundle.js && node_modules/.bin/mocha-phantomjs test/index.html"
  },
  "dependencies": {
    "browserify": "~3.30.1",
    "express": "~3.4.8",
    "react": "0.9.0-rc1",
    "react-tools": "0.9.0-rc1",
    "reactify": "~0.7.0",
    "url-pattern": "~0.3.1"
  },
  "devDependencies": {
    "mocha": "~1.17.1",
    "mocha-phantomjs": "~3.3.1",
    "phantomjs": "~1.9.7-1",
    "should": "~3.1.2",
    "supertest": "~0.9.0"
  }
}
